
{
    "presets": ["module:metro-react-native-babel-preset", "babel-preset-expo"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "@components": "./src/components",
            "@hooks": "./src/hooks",
            "@screens": "./src/screens",
            "@utils": "./src/utils"
          }
        }
      ]
    ]
  }
  